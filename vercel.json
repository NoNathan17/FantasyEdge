{
    "version": 2,
    "builds": [
    {
        "src": "fantasy_football_assistant/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.12.8"} 
    }
    ],
    "routes": [
    {
        "src": "/static/(.*)",
        "dest": "/static/$1"
        },
    {
        "src": "/(.*)",
        "dest": "fantasy_football_assistant/wsgi.py"
    }
    ]
  }
  